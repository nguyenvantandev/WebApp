function btnEvent(){
	var id = document.getElementById("kotobaid").value;
	if(id == ""){
		document.getElementById("actionStatus").value = "追加";
		document.getElementById("addupdate").submit();
	}else{
		document.getElementById("actionStatus").value = "更新";
		document.getElementById("addupdate").submit();
	}
}
